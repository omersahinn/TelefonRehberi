@model IEnumerable<Telefon_Rehberi.Calisanlar>

@{
    ViewBag.Title = "Index";
}

<h2>Çalışan Listesi</h2>


<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Adi)
        </th>
     
        <th>
            @Html.DisplayNameFor(model => model.Telefon)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.ActionLink(item.Adi, "Detay", new { id=item.CalisanId})
        </td>
       
        <td>
            @Html.DisplayFor(modelItem => item.Telefon)
        </td>
       
    </tr>
}

</table>
